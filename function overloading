#include <iostream>
using namespace std;
class Overload {
public:
 int add(int a, int b) {
 return a + b;
 }
 float add(float a, float b) {
 return a + b;
 }
};
int main() {
 Overload obj;
 int intResult;
 float floatResult;
 intResult = obj.add(10, 20);
 cout << "Sum of integers: " << intResult << endl;
 floatResult = obj.add(10.5f, 20.5f);
cout << "Sum of integers: " << intResult << endl;
 floatResult = obj.add(10.5f, 20.5f);
 cout << "Sum of floats: " << floatResult << endl;
 return 0;
}
